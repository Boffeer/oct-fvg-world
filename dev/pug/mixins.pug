mixin icon(name,mod)
	- mod = mod || ''
	svg(class="icon icon-" + name + ' ' + mod)
		use(xlink:href="static/img/svg/symbol/sprite.svg#" + name)



mixin card(delay, imgPath, text, number= false)
	- var cardNumber = imgPath.split('/')
	- var sliceCard = cardNumber.slice(1,2)
	- cardNumber = sliceCard
	div(class="card card--"+cardNumber, data-aos='fade-up', data-aos-delay =delay)
		picture(class='card-img card-img--'+cardNumber)
			img.card_img(
				src='static/img/content/'+imgPath+'.png',
				alt='')
		p.card-number
			if (text == 'сэкономил своим клиентам')
				span $
			if (number)
				span.cost-new(data-from="0" data-to=number)= number
				span +
		p.card-text= text

mixin cardHor(imgPath, text, arrowSide)
	.card-hor--wrap(class="card-hor--wrap-"+imgPath, data-aos="fade-up")
		div(class="card-hor--"+imgPath).card-hor
			picture.card-hor_picture
				img.card-hor_img(
					src='static/img/content/5-work/'+imgPath+'.png',
					alt='')
			p.card-hor_text= text
		if (arrowSide != 'none')
			img(src="static/img/general/arrow-"+arrowSide+".svg" alt="arrow" data-aos="fade-down-right").card-hor_arrow


mixin title(text, number, seciton)
	- var n = 1
	- var t = 1
	div(class='rolling-title '+seciton + '-title',
			id=seciton
			)
		while t < 4
			div(class='rolling-title-row--'+t class='title-row-'+t,
					id=seciton+'-title-row--'+t++)
				while n < number
					h2(class='rolling-title__item rolling-title__item-'+n++)= text
				- n= 1

// mixin case(numb, img, title, moscow, cost, duration, descr)
mixin case(data)
	// .case-wrap
		// div.case-modal_wrap(class='case-modal_wrap--'+ data.numb)
			// .case-modal
			// 	h3.case-modal_title= data.title
			// 	p.case-modal_text= data.descrFull
	.case-slide-wrap
		.case
			picture.case-pic
				img.img-loading(
					src='static/img/content/7-cases/'+data.pic+'.jpg',
					alt= data.title)
			.case-text
				h3.case-title= data.title
				.case-stats
					.case-row
						p Бюджет
						span.case-number= data.budget
					.case-row
						p Результат
						span.case-number= data.result
					.case-row
						p Срок
						span.case-number= data.time
				p.case-descr= data.descr
				button.g-secondary-btn.case-btn(class="case-btn--"+data.numb, data-case-number=data.numb data-button="Кейсы")
					span Подробнее →

mixin testimonial(name, title, picNum, text)
	.testimonial-wrap
		.testimonial
			div(class="testimonial-pic testimonial-pic--"+picNum)
				.testimonial-person
					h3.testimonial-name= name
					p.testimonial-title= title
			p.testimonial-text=text

mixin gdpr(btn)
	p.gdpr Нажимая кнопку «
		span= btn
		span », я даю согласие на передачу персональных данных согласно
			a.gdpr(href="#") политике конфиденциальности









