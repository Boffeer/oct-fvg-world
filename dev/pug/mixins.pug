mixin socials(data)
	div(class= data.class)
		if (data.inst)
			a.social--insta(href= data.inst, class= data.class+'__item')
				// img(src='static/img/general/ig.svg' alt="insta-link")
				+igIcon()
		if (data.vk)
			a.social--vk(href= data.vk, class= data.class+'__item')
				+vkIcon()

		if (data.fb)
			a.social--fb(href= data.fb, class= data.class+'__item')
				+fbIcon()
		if (data.tg)
			a.social--tg(href= data.tg, class= data.class+'__item')
				// img(src='static/img/general/tg.svg' alt="tg-link")
				+tgIcon()

		if (data.whats)
			a.social--whats(href= data.whats, class= data.class+'__item')
				+waIcon()
		if (data.vi)
			a.social--vi(href= data.vi, class= data.class+'__item')
				+viberIcon()